#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/xingw/Desktop/notchange/stalux/vuepress-starter/node_modules/.pnpm/vite@6.1.4_@types+node@22.14.0_sass-embedded@1.86.3/node_modules/vite/bin/node_modules:/mnt/c/Users/xingw/Desktop/notchange/stalux/vuepress-starter/node_modules/.pnpm/vite@6.1.4_@types+node@22.14.0_sass-embedded@1.86.3/node_modules/vite/node_modules:/mnt/c/Users/xingw/Desktop/notchange/stalux/vuepress-starter/node_modules/.pnpm/vite@6.1.4_@types+node@22.14.0_sass-embedded@1.86.3/node_modules:/mnt/c/Users/xingw/Desktop/notchange/stalux/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/xingw/Desktop/notchange/stalux/vuepress-starter/node_modules/.pnpm/vite@6.1.4_@types+node@22.14.0_sass-embedded@1.86.3/node_modules/vite/bin/node_modules:/mnt/c/Users/xingw/Desktop/notchange/stalux/vuepress-starter/node_modules/.pnpm/vite@6.1.4_@types+node@22.14.0_sass-embedded@1.86.3/node_modules/vite/node_modules:/mnt/c/Users/xingw/Desktop/notchange/stalux/vuepress-starter/node_modules/.pnpm/vite@6.1.4_@types+node@22.14.0_sass-embedded@1.86.3/node_modules:/mnt/c/Users/xingw/Desktop/notchange/stalux/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vite@6.1.4_@types+node@22.14.0_sass-embedded@1.86.3/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vite@6.1.4_@types+node@22.14.0_sass-embedded@1.86.3/node_modules/vite/bin/vite.js" "$@"
fi
